    "death": "^1.1.0",
    "dompurify": "^3.1.7",
    "esbuild": "^0.25.4",
    "express": "^4.18.2",
    "jsdom": "^25.0.1",
    "multer": "^1.4.5-lts.1",
    "tmp-promise": "^3.0.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/death": "^1.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
