  "description": "manages the image management and S3 cataloguing",
  "scripts": {
    "test": "jest -i",
    "testci": "jest --ci --bail -i",
    "lint": "eslint ./lib",
    "build": "rm -rf ./es5 && NODE_ENV=production tsc",
